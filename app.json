{
  "expo": {
    "name": "CampusXperience",
    "slug": "CampusXperience",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "config": {
        "usesCleartextTraffic": true,
        "googleServicesFile": "./google-services.json",
        "networkSecurityConfig": "./assets/network_security_config.xml"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.rohanudhwani.CampusXperience",
      "cleartext": {
        "config": {
          "cleartext": true
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "36355677-866b-49a8-81fb-b2a36fb1ac79"
      }
    },
    "plugins":[ [ "expo-build-properties", { "android": { "usesCleartextTraffic": true } } ] ]
  }
}
